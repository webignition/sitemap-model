{
    "name": "webignition/sitemap-model",
    "description": "Models a website sitemap",
    "keywords": ["sitemap"],
    "homepage": "https://github.com/webignition/sitemap-model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\WebResource\\Sitemap\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "webignition/web-resource-model":"1.1.9",
        "webignition/url":">=0.1,<2.0",
        "simplepie/simplepie":"1.5",
        "hobnob/xml-stream-reader":"1.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "guzzle/guzzle":">=3.8.1,<4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "minimum-stability":"dev"
}