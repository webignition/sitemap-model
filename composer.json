{
    "name": "webignition/sitemap-model",
    "description": "Models a website sitemap",
    "keywords": ["sitemap"],
    "homepage": "https://github.com/webignition/sitemap-model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\WebResource\\Sitemap\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2.0",
        "webignition/web-resource-interfaces":">=3.2,<4",
        "webignition/web-resource-model":">=4,<5",
        "webignition/url":">=2,<3",
        "simplepie/simplepie":"1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3",
        "mockery/mockery": "^1",
        "webignition/web-resource-testing-tools":"^0.2"
    },
    "minimum-stability":"stable"
}